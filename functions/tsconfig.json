{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "compileOnSave": true,
  "include": [
    "src",
    "utilities/extract-video-frames.ts",
    "utilities/multipart.ts",
    "utilities/handle-on-request-error.ts",
    "utilities/generate-verification-code.ts",
    "utilities/send-otp-code-email.ts",
    "utilities/email-templates/generate-otp-code-template.ts",
    "utilities/__mocks__/scan-categories.ts",
    "utilities/generate-unique-id.ts",
    "utilities/truncate-email-address.ts",
    "dayjs/index.ts"
  ]
}
